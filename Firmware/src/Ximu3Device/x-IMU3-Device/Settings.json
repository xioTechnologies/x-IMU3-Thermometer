{
    "includes": [
        "<stdbool.h>",
        "<stdint.h>"
    ],
    "settings": [
        {
            "name": "Serial number",
            "declaration": "char name[32]",
            "default": "{\"Unknown\"}",
            "preserved": true
        },
        {
            "name": "Hardware version",
            "declaration": "char name[32]",
            "default": "{\"Unknown\"}",
            "preserved": true
        },
        {
            "name": "Firmware version",
            "declaration": "char name[32]",
            "default": "{\"Unknown\"}",
            "read-only": true
        },
        {
            "name": "Device name",
            "declaration": "char name[32]",
            "default": "{\"x-IMU3 Device\"}"
        },
        {
            "name": "Serial enabled",
            "declaration": "bool name",
            "default": "{true}"
        },
        {
            "name": "Serial baud rate",
            "declaration": "uint32_t name",
            "default": "{115200}"
        },
        {
            "name": "Serial RTS/CTS enabled",
            "declaration": "bool name",
            "default": "{false}"
        },
        {
            "name": "Binary mode enabled",
            "declaration": "bool name",
            "default": "{true}"
        },
        {
            "name": "USB data messages enabled",
            "declaration": "bool name",
            "default": "{true}"
        },
        {
            "name": "Serial data messages enabled",
            "declaration": "bool name",
            "default": "{true}"
        },
        {
            "name": "Example float",
            "declaration": "float name",
            "default": "{1.0f}"
        }
    ]
}